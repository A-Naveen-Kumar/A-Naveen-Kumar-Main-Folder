using System;
using System.IO;
using ebmsFramework;

namespace FRTestApp
{
    class Program
    {
        static void Main(string[] args)
        {
            try
            {
                SFPDFEngine pdfEngine = new SFPDFEngine();
                pdfEngine.Initialize(false);
                pdfEngine.WriteCompanyDetails();
                pdfEngine.WritePageNumbers();
                pdfEngine.AddPlainLine("HAI, ABCDEFGHIJKLMNOPQRSTUVWXYZ, abcdefghijklmnopqrstuvwxyz!", 100, 100, PrintFont.Courier10);
                pdfEngine.AddPlainLine("HAI, ABCDEFGHIJKLMNOPQRSTUVWXYZ, abcdefghijklmnopqrstuvwxyz!", 100, 110, PrintFont.U1);
                pdfEngine.AddPlainLine("HAI, ABCDEFGHIJKLMNOPQRSTUVWXYZ, abcdefghijklmnopqrstuvwxyz!", 100, 120, PrintFont.H1);
                pdfEngine.AddPlainLine("HAI, ABCDEFGHIJKLMNOPQRSTUVWXYZ, abcdefghijklmnopqrstuvwxyz!", 100, 130, PrintFont.H2);
                pdfEngine.AddPlainLine("HAI, ABCDEFGHIJKLMNOPQRSTUVWXYZ, abcdefghijklmnopqrstuvwxyz!", 100, 140, PrintFont.H3);
                pdfEngine.AddPlainLine("HAI, ABCDEFGHIJKLMNOPQRSTUVWXYZ, abcdefghijklmnopqrstuvwxyz!", 100, 150, PrintFont.TimesRoman8);
                pdfEngine.AddPlainLine("HAI, ABCDEFGHIJKLMNOPQRSTUVWXYZ, abcdefghijklmnopqrstuvwxyz!", 100, 160, PrintFont.TimesRoman10BOLD);
                pdfEngine.BarCode("www.ebms.co.in", 100, 250);
                RectangleDimensions rect = new RectangleDimensions(200, 250, 100, 100);
                pdfEngine.AddRectangle("<= Scan this QR CODE to visit www.ebms.co.in", PrintFont.B1, rect);

                pdfEngine.WriteCompanyDetailsEveryPage();
                pdfEngine.AddMultiSectionLine((PrintLineSection)3);
                
                var result = pdfEngine.GeneratePDF();
                FileStream fStream = new FileStream("D:\\PrintOutputs\\New\\NAVEEN.pdf", FileMode.CreateNew);
                result.stream.WriteTo(fStream);
                fStream.Close();
                Console.WriteLine("PDF - Created Succesfully!!!");
            }
            catch (Exception e)
            {
                Console.WriteLine("Error - Occured!!!\n" + e.Message);
            }
        }
    }
}
